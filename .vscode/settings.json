{
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true,
    "editor.formatOnPaste": false,
    "[python]": {
        "editor.defaultFormatter": "ms-python.black-formatter",
        "editor.formatOnSave": true
    },
    "search.exclude": {
        "**/.git": true,
        "**/cypress/results": true,
        "**/downloads": true,
        "**/mochawesome*": true,
        "**/node_modules": true,
        "**/test-results": true,
        "**/test-reports*": true,
        "**/screenshots": true
    }
}
